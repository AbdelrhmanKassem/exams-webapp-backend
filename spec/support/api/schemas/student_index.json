    "students": {
      "type": "array",
      "items": {
        "required": ["id", "full_name", "seat_number", "branch", "school"],
        "type": "object",
        "properties": {
            "id": {"type":"integer"},
            "full_name": {"type":"string"},
            "email": {"type":["string", "null"]},
